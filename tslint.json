{
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": false,
    "member-access": [
      true,
      "no-public"
    ],
    "no-empty": false,
    "object-literal-sort-keys": false,
    "one-line": false,
    "trailing-comma": false
  }
}